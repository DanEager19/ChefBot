{
  "name": "chef-bot",
  "version": "1.0.0",
  "description": "Cooking and Gardening Club Discord bot reminder.<br />\r Currently Hosted on a Raspberry Pi<br />",
  "main": "index.ts",
  "scripts": {
    "start:dev": "ts-node src/deploy-commands.ts && ts-node src/index.ts",
    "start:prod": "node dist/deploy-commands.js && node dist/index.js",
    "build": "tsc --build",
    "build:container": "docker build -t chefbot ."
  },
  "dependencies": {
    "@discordjs/builders": "^1.0.0",
    "@discordjs/rest": "^1.0.0",
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.8.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^18.0.1",
    "@typescript-eslint/parser": "^5.30.5",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
